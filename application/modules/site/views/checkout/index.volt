<body class="page-template page-template-page-templates page-template-template-fluid page-template-page-templatestemplate-fluid-php page page-id-27 logged-in custom-background woocommerce-checkout woocommerce-page woocommerce-demo-store woocommerce-no-js conj-has-social__menu conj-has-secondary__menu has-sidebar woocommerce-running conj-header-logo__left conj-social-menu__left conj-secondary-menu__right conj-primary-menu__left left-sidebar conj-copyright-content__left conj-wc-checkout-columns conj-hide-wc-checkout-distractions conj-wc-two-step__checkout">

<div id="conj-site__wrapper">
    <div id="page" class="hfeed site c-offcanvas-content-wrap">

        <header id="masthead" class="site-header jarallax" role="banner" data-jarallax data-img-element="jarallax-header-img" data-speed="1">
            <div class="col-full">

                <div class="site-branding">
                    <p class="beta site-title">
                        <a href="{{ url.getBaseUri() }}" rel="home">
                            <img src="https://lh3.googleusercontent.com/-8Frbiyi3m60/W0Zl9h2JDiI/AAAAAAAAD7k/RITqPTrVPhYbX1X4MIIMi-9MGmFmPaiOQCL0BGAYYCw/h55/2018-07-11.png" alt="">
                        </a>
                    </p>
                </div>

            </div>
        </header>

        <div id="content" class="site-content" tabindex="-1">

            <div class="conj-col__full" id="app" data-noint="{{ installmentsnointerest }}">

                <div id="primary" class="content-area">
                    <main id="main" class="site-main" role="main">

                        <article id="post-27" class="post-27 page type-page status-publish hentry">

                            <transition name="zoomIn" mode="in-out">
                                <div class="entry-content" v-if="!response.success">
                                    <div class="woocommerce">

                                        <form name="checkout" method="post" id="checkout_form" class="checkout woocommerce-checkout" :d="canGenerateCardToken">

                                            <div class="checkout-slides">
                                                <ul class="checkout-control-nav">
                                                    <li><a href="#">Informações</a></li>
                                                    <li><a href="#">Pagamento</a></li>
                                                </ul>
                                                <ul class="conj-wc-two-step-checkout">

                                                    <li class="conj-wc-addresses">

                                                        <div class="col2-set" id="customer_details">
                                                            <div class="col-1">
                                                                <div class="woocommerce-billing-fields">
                                                                    <h3>Informações básicas</h3>

                                                                    <input id="new_address" type="checkbox" v-model="address.different">
                                                                    <label for="new_address" style="color: #333; font-size: 20px; font-weight: bold;"> Entregar em outro endereço? </label>

                                                                    <div v-show="!address.different" class="woocommerce-billing-fields__field-wrapper">
                                                                        <p class="form-row form-row-first validate-required" data-priority="10">
                                                                            <label for="billing_first_name" class="">Nome</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="text" class="input-text" value="{{ usuario.nome }}"/>
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-last validate-required" data-priority="20">
                                                                            <label for="billing_last_name" class="">Sobrenome</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="text" class="input-text" value="{{ usuario.sobrenome }}" />
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-wide" data-priority="30">
                                                                            <label for="billing_company" class="">CPF</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="text" class="input-text" value="{{ usuario.documento["valor"] }}"/>
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-first address-field validate-required" data-priority="50">
                                                                            <label for="billing_address_1" class="">CEP</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="text" class="input-text " value="{{ usuario.endereco["cep"] }}" />
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-last address-field validate-required" data-priority="50">
                                                                            <label for="billing_address_1" class="">Número</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="text" class="input-text " value="{{ usuario.endereco["numero"] }}" />
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-first address-field"  data-priority="60">
                                                                            <label for="billing_address_1" class="">Bairro</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="text" class="input-text " value="{{ usuario.endereco["bairro"] }}" />
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-last address-field validate-required" data-priority="70">
                                                                            <label for="billing_city" class="">Rua</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="text" class="input-text " value="{{ usuario.endereco["rua"] }}"/>
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-first address-field validate-required validate-postcode" data-priority="90">
                                                                            <label for="billing_postcode" class="">Cidade</label>
                                                                            <span class="woocommerce-input-wrapper">
			                                                                <input disabled type="text" class="input-text " value="{{ usuario.endereco["cidade"] }}" />
	                                                                	</span>
                                                                        </p>
                                                                        <p class="form-row form-row-last validate-required validate-phone" data-priority="100">
                                                                            <label for="billing_phone" class="">Estado</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="tel" class="input-text " value="{{ usuario.endereco["estado"] }}"/>
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-wide validate-required validate-phone" data-priority="100">
                                                                            <label for="billing_phone" class="">Complemento</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="tel" class="input-text " value="{{ usuario.endereco["complemento"] }}"/>
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-wide validate-required validate-email" data-priority="110">
                                                                            <label for="billing_email" class="">Email</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled type="email" class="input-text " value="{{ usuario.email }}"/>
                                                                        </span>
                                                                        </p>
                                                                    </div>

                                                                    <div v-if="address.different" class="woocommerce-billing-fields__field-wrapper">
                                                                        <p class="form-row form-row-first address-field validate-required" data-priority="50">
                                                                            <label for="billing_address_1" class="">CEP</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input v-model="address.data.cep" type="text" class="input-text " value="{{ usuario.endereco["cep"] }}" />
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-last address-field validate-required" data-priority="50">
                                                                            <label for="billing_address_1" class="">Número</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input v-model="address.data.numero" type="text" class="input-text " value="{{ usuario.endereco["numero"] }}" />
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-first address-field"  data-priority="60">
                                                                            <label for="billing_address_1" class="">Bairro</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled v-model="address.data.bairro" type="text" class="input-text " value="{{ usuario.endereco["bairro"] }}" />
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-last address-field validate-required" data-priority="70">
                                                                            <label for="billing_city" class="">Rua</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled v-model="address.data.rua" type="text" class="input-text " value="{{ usuario.endereco["rua"] }}"/>
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-first address-field validate-required validate-postcode" data-priority="90">
                                                                            <label for="billing_postcode" class="">Cidade</label>
                                                                            <span class="woocommerce-input-wrapper">
			                                                                <input disabled v-model="address.data.cidade" type="text" class="input-text " value="{{ usuario.endereco["cidade"] }}" />
	                                                                	</span>
                                                                        </p>
                                                                        <p class="form-row form-row-last validate-required validate-phone" data-priority="100">
                                                                            <label for="billing_phone" class="">Estado</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input disabled v-model="address.data.estado" type="tel" class="input-text " value="{{ usuario.endereco["estado"] }}"/>
                                                                        </span>
                                                                        </p>
                                                                        <p class="form-row form-row-wide validate-required validate-phone" data-priority="100">
                                                                            <label for="billing_phone" class="">Complemento</label>
                                                                            <span class="woocommerce-input-wrapper">
                                                                            <input v-model="address.data.complemento" type="tel" class="input-text " value="{{ usuario.endereco["complemento"] }}"/>
                                                                        </span>
                                                                    </div>

                                                                </div>

                                                            </div>

                                                            <div class="col-2">
                                                                <div class="woocommerce-shipping-fields">

                                                                    <h3 id="ship-to-different-address">
                                                                        <label class="woocommerce-form__label woocommerce-form__label-for-checkbox checkbox">
                                                                            <span>Pedido</span>
                                                                        </label>
                                                                    </h3>

                                                                    <div class="shipping_address">
                                                                        <table class="woocommerce-checkout-review-order-table">
                                                                            <thead>
                                                                            <tr>
                                                                                <th class="product-name">Produtos</th>
                                                                                <th class="product-total">Total</th>
                                                                            </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                            {% for item in carrinho %}
                                                                                <tr class="cart_item">
                                                                                    <td class="product-name">
                                                                                        {{ item["nome"] }}
                                                                                        <strong class="product-quantity">&times; {{ item["quantity"] }}</strong>
                                                                                    </td>
                                                                                    <td class="product-total">
                                                                                    <span class="woocommerce-Price-amount amount"><span class="woocommerce-Price-currencySymbol">R&#36;</span>
                                                                                        {{ item["preco"] *item["quantity"] }}
                                                                                    </span>
                                                                                    </td>
                                                                                </tr>
                                                                            {% endfor %}
                                                                            </tbody>
                                                                            <tfoot>

                                                                            <tr class="cart-subtotal">
                                                                                <th>Subtotal</th>
                                                                                <td>
                                                                                <span class="woocommerce-Price-amount amount">
                                                                                    <span id="subtotalpurcharse" data-subtotal="{{ totalcarrinho.total }}" class="woocommerce-Price-currencySymbol">R&#36;
                                                                                    </span>{{ totalcarrinho.total }}
                                                                                </span>
                                                                                </td>
                                                                            </tr>

                                                                            <tr class="shipping">
                                                                                <th>Frete</th>
                                                                                <td data-title="Shipping">
                                                                                    <ul id="shipping_method">
                                                                                        {% for key,f in frete %}
                                                                                            {% if f["error"]|length === 0 %}
                                                                                                <li>
                                                                                                    <input data-type="{{ f["name"] }}" data-price="{{ f["price"] }}" type="radio" name="shipping_method"
                                                                                                           data-index="{{ key }}"
                                                                                                           id="shipping_method_0_flat_shipping{{ key }}"
                                                                                                           value="{{ f["name"] }}:{{ f["price"] }}:{{ f["deadline"] }}"
                                                                                                           class="shipping_method radio"  checked='checked' />
                                                                                                    <label for="shipping_method_0_flat_shipping{{ key }}">{{ f["name"] }} ( {{ f["deadline"] }} dias úteis) :
                                                                                                        <span class="woocommerce-Price-amount amount">
                                                                                                    <span class="woocommerce-Price-currencySymbol">R&#36;</span>{{ f["price"] }}
                                                                                                </span>
                                                                                                    </label>
                                                                                                </li>
                                                                                            {% endif %}
                                                                                        {% endfor %}
                                                                                    </ul>

                                                                                </td>
                                                                            </tr>

                                                                            <tr class="order-total">
                                                                                <td>
                                                                                    <strong>
                                                                                    <span id="totalvalue" class="woocommerce-Price-amount amount">
                                                                                        <span class="woocommerce-Price-currencySymbol">R&#36;</span>{{ "{{ total }}" }}
                                                                                    </span>
                                                                                    </strong>
                                                                                </td>
                                                                            </tr>

                                                                            </tfoot>
                                                                        </table>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>

                                                    </li>

                                                    <li class="order-review">

                                                        <div id="order_review" class="woocommerce-checkout-review-order">
                                                            <div id="payment" class="woocommerce-checkout-payment">
                                                                <ul class="wc_payment_methods payment_methods methods">
                                                                    {% for method in methods %}
                                                                        {% if method === 'boleto' %}
                                                                            <li class="wc_payment_method payment_method_boleto">
                                                                                <input value="boleto" v-model="method" id="payment_method_boleto" type="radio" class="input-radio" name="payment_method"
                                                                                       checked="checked">
                                                                                <label for="payment_method_boleto">Boleto</label>
                                                                                <small>+R$1</small>
                                                                            </li>
                                                                        {% endif %}
                                                                        {% if method === 'cartao_credito' %}
                                                                            <li class="wc_payment_method payment_method_stripe">
                                                                                <input value="credit_card" v-model="method" id="payment_method_stripe" type="radio" class="input-radio"
                                                                                       name="payment_method">
                                                                                <label for="payment_method_stripe"> Cartão de crédito </label>
                                                                                <div class="payment_box payment_method_stripe" style="" v-if="method !== 'boleto'">
                                                                                    <div id="stripe-payment-data">

                                                                                        <fieldset id="wc-stripe-cc-form" class="wc-credit-card-form wc-payment-form" style="background:transparent;">
                                                                                            <div class="form-row form-row-wide">
                                                                                                <label for="stripe-card-element">Número do cartão
                                                                                                    <span class="required">*</span>
                                                                                                    <small v-text="card.brand"></small>
                                                                                                </label>
                                                                                                <input maxlength="16" v-model="card.number" type="number" class="input-text" placeholder="1111 1111 1111 1111">
                                                                                            </div>
                                                                                            <div v-if="card.brand" class="form-row form-row-wide">
                                                                                                <div class="form-row form-row-first">
                                                                                                    <label for="stripe-cvc-element">Código de segurança
                                                                                                        <span class="required">*</span>
                                                                                                    </label>
                                                                                                    <input v-model="card.cvv" type="number" class="input-text" placeholder="123">
                                                                                                </div>
                                                                                                <div v-if="card.cvv.length === 3" class="form-row form-row-last">
                                                                                                    <div class="form-row form-row-first">
                                                                                                        <label for="stripe-cvc-element">Mês
                                                                                                            <span class="required">*</span>
                                                                                                        </label>
                                                                                                        <input v-model="card.expiration.month" type="number" class="input-text" placeholder="12">
                                                                                                    </div>
                                                                                                    <div v-if="card.expiration.month.length === 2" class="form-row form-row-last">
                                                                                                        <label for="stripe-cvc-element">Ano
                                                                                                            <span class="required">*</span>
                                                                                                        </label>
                                                                                                        <input v-model="card.expiration.year" type="number" class="input-text" placeholder="24">
                                                                                                    </div>
                                                                                                </div>
                                                                                                <div v-if="installments.options.length > 0" class="form-row form-row-wide">
                                                                                                    <label for="stripe-card-element">Parcelas
                                                                                                        <span class="required">*</span>
                                                                                                    </label>
                                                                                                    <select name="" id="" v-model="installments.current" :disabled="!installments.options">
                                                                                                        <option value="" disabled>Selecione a parcela</option>
                                                                                                        <option v-for="option in installments.options" :value="`${option.quantity}:${option
                                                                                            .installmentAmount}`" :key="JSON.stringify(option)">
                                                                                                            {{ "{{ option.quantity }}x de {{ option.installmentAmount }}. {{ !option.interestFree ?
                                                                                                    `Acréscimo de
                                                                                                R$ `+(option.totalAmount - total).toFixed(2) : '' }}" }}
                                                                                                        </option>
                                                                                                    </select>
                                                                                                </div>
                                                                                            </div>
                                                                                            <div class="clear"></div>
                                                                                            <div id="logs_error_credit_card"></div>
                                                                                            <div class="clear"></div>
                                                                                        </fieldset>
                                                                                    </div>
                                                                                </div>
                                                                            </li>
                                                                        {% endif %}
                                                                    {% endfor %}
                                                                </ul>
                                                            </div>
                                                            <div id="payment" class="woocommerce-checkout-payment">
                                                                <div class="form-row place-order">
                                                                    <span v-if="method !== 'boleto'" style="color: #FF4961;">Preencha todos campos para finalizar a compra</span>
                                                                    <div class="loading" v-if="response.loading"></div>
                                                                    <button data-session="{{ pagsession }}" type="submit" class="button alt"
                                                                            name="woocommerce_checkout_place_order" id="place_order" data-value="Place order" :disabled="checkDisabled">
                                                                        {{ "{{ response.loading ? 'Concluindo compra...' : 'Finalizar compra' }}" }}
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </li>

                                                </ul>
                                            </div>

                                        </form>

                                    </div>
                                </div>
                            </transition>

                            <transition name="zoomIn" mode="out-in">
                                <div class="payment-finished" v-if="response.success">
                                    <div class="woocommerce-checkout-review-order">
                                        <div class="woocommerce-checkout-payment">
                                            <hgroup style="text-align: center;">
                                                <h1>COMPRA FINALIZADA</h1>
                                                <h2>Sua compra foi finalizada com sucesso!</h2>
                                            </hgroup>

                                            <p>Tipo de pagamento: <strong>{{ "{{ method === 'credit_card' ? 'Cartão de crédito' : method }}" }}</strong></p>
                                            <p>Código da transação: {{ "{{ response.params.code }}" }}</p>
                                            <a v-if="method === 'boleto'" :href="response.params.link" target="_blank">Link para pagamento</a>

                                        </div>
                                        <div id="payment" class="woocommerce-checkout-payment">
                                            <div class="form-row place-order">
                                                <a href="{{ url.getBaseUri() }}" class="button alt" name="woocommerce_checkout_place_order" id="place_order">
                                                    Voltar para a loja
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </transition>

                        </article>
                    </main>
                </div>

            </div>

        </div>

    </div>

</div>

